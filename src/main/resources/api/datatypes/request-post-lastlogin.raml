#%RAML 1.0 DataType
type: object
properties:
  deviceId:
    required: true
    type: string
    maxLength: 50
  lastLogin:
    required: true
    type: datetime-only
  appVersion:
    required: true
    maxLength: 50
  individualId:
    type: string
    required: true
    pattern: ^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$
  partyId:
    type: string
    required: true
    maxLength: 100